pipeline {
	agent {
		node {
			label "built-in"
				customWorkspace "/mnt/replica/"
		}
	}
	stages {
		stage ('install') {
			steps {
				sh "yum install git -y"
			}
		}
		stage ('check') {
			steps {
				sh "git --version"
			}
		}
		stage ('file-creat') {
			steps {
				sh "echo 'git is back' >> jubli"
			}
		}	
	}	
}	
